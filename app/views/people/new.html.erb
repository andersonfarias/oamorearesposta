<%= simple_form_for(@person) do |f| %>
  <%= f.error_notification %>
  <br/>
  <div class="row">
    <%= render 'form', f: f %>
  </div>
  <div class="ln_solid"></div>
  <div class="form-group">
    <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3 text-center">
      <%= f.button :submit, class: "btn btn-primary" %>
    </div>
  </div>
<% end %>
