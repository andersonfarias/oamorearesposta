<Row>
	<Cell ss:StyleID="title2" ><Data ss:Type="String">1</Data></Cell>
	<Cell ss:StyleID="title2" ><Data ss:Type="String">Dados Gerais</Data></Cell>
</Row>

<Row>
	<Cell><Data ss:Type="String">1.1</Data></Cell>
	<Cell><Data ss:Type="String">Número Folha</Data></Cell>
	<% evaluations.each do |e| %>
		<Cell><Data ss:Type="String"><%= e.number %></Data></Cell>
	<% end %>
</Row>

<Row>
	<Cell><Data ss:Type="String">1.1</Data></Cell>
	<Cell><Data ss:Type="String">Parceiro (apenas as iniciais)</Data></Cell>
	<% evaluations.each do |e| %>
		<Cell><Data ss:Type="String"><%= e.beneficiary.person.initials %></Data></Cell>
	<% end %>	
</Row>

<Row>
	<Cell><Data ss:Type="String">1.3</Data></Cell>
	<Cell><Data ss:Type="String">Código da organização</Data></Cell>
	<% evaluations.each do |e| %>
		<Cell><Data ss:Type="String"><%= e.organization_code %></Data></Cell>
	<% end %>	
</Row>

<Row>
	<Cell><Data ss:Type="String">1.4</Data></Cell>
	<Cell><Data ss:Type="String">Data do primeiro contato</Data></Cell>
	<% evaluations.each do |e| %>
		<Cell><Data ss:Type="String"><%= e.beneficiary.first_contact_file.date.strftime('%d/%m/%Y') %></Data></Cell>
	<% end %>
</Row>

<Row>
	<Cell><Data ss:Type="String">1.5</Data></Cell>
	<Cell><Data ss:Type="String">Data de início do tratamento Comunitário</Data></Cell>
	<% evaluations.each do |e| %>
		<Cell><Data ss:Type="String"><% if !e.beneficiary.clinic_treatments.nil? and !e.beneficiary.clinic_treatments.empty? %><%= e.beneficiary.clinic_treatments.first.date.strftime('%d/%m/%Y').strip %><% end%></Data></Cell>
	<% end %>
</Row>

<Row>
	<Cell><Data ss:Type="String">1.6</Data></Cell>
	<Cell><Data ss:Type="String">Data de conclusão do tratamento Comunitário</Data></Cell>
</Row>

<Row>
	<Cell><Data ss:Type="String">1.7</Data></Cell>
	<Cell><Data ss:Type="String">Interrupções (data)</Data></Cell>
	</Row>

<Row>
	<Cell><Data ss:Type="String">1.8</Data></Cell>
	<Cell><Data ss:Type="String">Processo concluído</Data></Cell>
</Row>

<Row>
	<Cell><Data ss:Type="String">1.9</Data></Cell>
	<Cell><Data ss:Type="String">Processo em desenvolvimento</Data></Cell>
	<% evaluations.each do |e| %>
		<Cell><Data ss:Type="String"><%= t "#{e.developing_process}" %></Data></Cell>
	<% end %>
</Row>

<Row>
	<Cell><Data ss:Type="String">1.10</Data></Cell>
	<Cell><Data ss:Type="String">Suspenções</Data></Cell>
	<% evaluations.each do |e| %>
		<Cell><Data ss:Type="String"><%= t "#{e.suspensions}" %></Data></Cell>
	<% end %>	
</Row>

<Row>
	<Cell><Data ss:Type="String">1.11</Data></Cell>
	<Cell><Data ss:Type="String">Data da avaliação</Data></Cell>
	<% evaluations.each do |e| %>
		<Cell><Data ss:Type="String"><%= e.evaluation_date.strftime('%d/%m/%Y') %></Data></Cell>
	<% end %>	
</Row>