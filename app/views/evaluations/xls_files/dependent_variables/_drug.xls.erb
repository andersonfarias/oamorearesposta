<Row>
	<Cell ss:StyleID="title3" ><Data ss:Type="String">5</Data></Cell>
	<Cell ss:StyleID="title3" ><Data ss:Type="String"><%= t('activerecord.attributes.dependent_variable.drug') %></Data></Cell>
</Row>

<% @data = YAML.load(File.open("#{Rails.root}/config/locales/models/#{I18n.locale}.yml")) %>
<% @data["#{I18n.locale}"]["activerecord"]["attributes"]["drug"].each do |key, value| %>
	<Row>
		<Cell><Data ss:Type="String"><%= key.sub("i_", "").gsub("_",".") %></Data></Cell>
		<Cell><Data ss:Type="String"><%= value %></Data></Cell>
		<% evaluations.each do |e| %>
			<Cell><Data ss:Type="String"><%= e.dependent_variable.drug.read_attribute(key) %></Data></Cell>
		<% end %>
	</Row>
<% end %>