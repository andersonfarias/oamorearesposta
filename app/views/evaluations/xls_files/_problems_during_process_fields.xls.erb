<Row>
	<Cell ss:StyleID="title2" ><Data ss:Type="Number">2</Data></Cell>
	<Cell ss:StyleID="title2" ><Data ss:Type="String"><%= t('activerecord.attributes.evaluation.problems_during_process') %></Data></Cell>
</Row>

<% number = 0%>
<% @data = YAML.load(File.open("#{Rails.root}/config/locales/models/#{I18n.locale}.yml")) %>
<% @data["#{I18n.locale}"]["activerecord"]["attributes"]["problems_during_process"].each do |key, value| %>
	<Row>
		<Cell><Data ss:Type="String">2.<%= number += 1 %></Data></Cell>
		<Cell><Data ss:Type="String"><%= value %></Data></Cell>
		<% evaluations.each do |e| %>
			<Cell><Data ss:Type="String"><%= e.problems_during_process.read_attribute(key) %></Data></Cell>
		<% end %>
	</Row>
<% end %>



